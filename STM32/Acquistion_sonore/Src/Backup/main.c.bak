uint32_t moyenne = 0x800;
char a = 0;
float ret;
uint32_t somme50 = 0;
uint32_t ADCValue;
uint32_t seuil = 0x00F;
uint32_t Vdetect = 0xF000;

void HAL_ADC_ConvCpltCallback(ADC_HandleTypeDef* hadc1){
	
	if (hadc1->Instance == ADC1){
		value = HAL_ADC_GetValue(hadc1);
	}
}

void HAL_TIM_PeriodElapsedCallback(TIM_HandleTypeDef* htim){
	HAL_ADC_Start(&hadc2);
	HAL_ADC_PollForConversion(&hadc2,10);
	ADCValue = HAL_ADC_GetValue(&hadc2);
	
	if(i<100){
		if(ADCValue<seuil){
			somme50 += seuil-ADCValue;
		}
		else{
			somme50 += ADCValue -seuil;
		}
		i++;
	}
	else{
		if (ADCValue == 0x000){
			HAL_DAC_SetValue(&hdac,DAC_CHANNEL_1,DAC_ALIGN_12B_R,0x000);
		}
		else{
			if(somme50>Vdetect){
				HAL_DAC_SetValue(&hdac,DAC_CHANNEL_1,DAC_ALIGN_12B_R,0xFFFF);
			}
			else{
				HAL_DAC_SetValue(&hdac,DAC_CHANNEL_1,DAC_ALIGN_12B_R,0x0000);
			}
			HAL_DAC_Start(&hdac,DAC_CHANNEL_1);
			i=0;
			somme50 = 0;
		}
		
		
		  if (a<10)
			{
				HAL_GPIO_WritePin (GPIOE, LD3_Pin, GPIO_PIN_RESET);
				a+=1;
			}
			else
			{
				HAL_GPIO_WritePin (GPIOE, LD3_Pin, GPIO_PIN_SET);
				if (a>20)
				{
					a=0;
				}
				else
				{
					a+=1;
				}
			}


}
